#!/bin/sh
JLINK_VM_OPTIONS="--add-exports javafx.base/com.sun.javafx=ALL-UNNAMED --add-exports javafx.graphics/com.sun.javafx.stage=ALL-UNNAMED --add-exports javafx.graphics/com.sun.javafx.tk=ALL-UNNAMED --add-opens javafx.graphics/javafx.scene=ALL-UNNAMED --add-opens java.base/java.util=ALL-UNNAMED --add-exports javafx.graphics/com.sun.javafx.css=ALL-UNNAMED --add-opens javafx.graphics/com.sun.javafx.css=ALL-UNNAMED"
DIR=`dirname $0`
$DIR/java $JLINK_VM_OPTIONS -m thito.nodeflow/thito.nodeflow.launcher.Main "$@"
