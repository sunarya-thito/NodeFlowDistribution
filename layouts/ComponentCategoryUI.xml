<NodeFlow>
    <components>
        <ImageView>javafx.scene.image.ImageView</ImageView>
        <HBox>javafx.scene.layout.HBox</HBox>
        <VBox>javafx.scene.layout.VBox</VBox>
        <Label>javafx.scene.control.Label</Label>
    </components>
    <layout>
        <HBox class="node-category">
            <ImageView class="node-category-icon"/>
            <VBox class="node-category-description">
                <Label id="node-category-alias"/>
                <Label id="node-category-name"/>
            </VBox>
        </HBox>
    </layout>
</NodeFlow>